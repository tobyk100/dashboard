# Ignore bundler config.
/.bundle

# Ignore the default SQLite database.
/db/*.sqlite3
/db/*.sqlite3-journal

# Ignore all logfiles and tempfiles.
/log/*.log
/tmp

# ignore rubymine files
/.idea

# Ignore application & generated configuration
/config/application.yml
/config/unicorn.rb
/config/nginx.conf

# Ignore apple fs files
.DS_Store

# asset pipeline files
/public/assets

# binstub files
/bin

# Ignore per-user Vagrant data
.vagrant

# Fetched from YouTube
/public/c

# Generated by rake
/config/locales/*en-PLOC.yml
